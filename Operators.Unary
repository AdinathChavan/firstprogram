package Javatutorials.com.Javatutorials.com.Javatutorials.operators;

public class unary {
    public static void main(String[] args)
    {
        int a = 224;
        System.out.println(++a);
        System.out.println(a++);
        System.out.println(a);
        System.out.println(a+1);

    }
}
