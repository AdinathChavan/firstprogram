package com.company;
import java.util.Scanner;

public class forloop {
    public static void main(String[] args)
    {
        System.out.println("Enter the number that you want table of the number");
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int i = 1;
        for(i=1; i<=10; i++)
        {
            int mul = i*a;
            System.out.println(mul);
        }
    }
}
