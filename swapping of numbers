package Javatutorials;

public class swap {
    public static void swapping(int a,int b){
        int c; 
        c =a;
        a=b;
        b=c;
        System.out.println("value of a " +a);
        System.out.println("value of b " +b);
    }
    public static void main(String[] args){
        int  a = 233;
        int b = 342;
        swapping(a,b);

    }
}
